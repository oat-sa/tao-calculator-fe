{
  "name": "tao-calculator",
  "version": "0.0.0",
  "description": "A calculator's engine for TAO",
  "main": "src/index.js",
  "files": [
    "src",
    "!**/test/"
  ],
  "scripts": {
    "test": "jest --config=./jest.config.cjs",
    "test:watch": "npm test -- --watch",
    "test:update": "npm test -- --updateSnapshot",
    "test:cov": "npm test -- --coverage",
    "coverage:html": "open-cli .coverage/lcov-report/index.html",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore --ignore-path .eslintignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oat-sa/tao-calculator-fe.git"
  },
  "keywords": [
    "TAO",
    "calculator"
  ],
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/oat-sa/tao-calculator-fe/issues"
  },
  "homepage": "https://github.com/oat-sa/tao-calculator-fe#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@oat-sa/eslint-config-tao": "^1.2.0",
    "@oat-sa/expr-eval": "^1.3.1",
    "@oat-sa/prettier-config": "^0.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "babel-jest": "^29.3.1",
    "decimal.js": "^10.4.3",
    "eslint": "^8.32.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsdoc": "^39.6.4",
    "handlebars": "^4.7.7",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-transform-stub": "^2.0.0",
    "lodash": "^4.17.21",
    "moo": "^0.5.2",
    "open-cli": "^7.1.0",
    "prettier": "^2.8.3"
  },
  "prettier": "@oat-sa/prettier-config"
}
